{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": "build/web",
  "installCommand": "echo 'Skipping install, using GitHub Actions build'",
  "framework": null,
  "rewrites": [
    { "source": "/assets/:path*", "destination": "/assets/:path*" },
    { "source": "/canvaskit/:path*", "destination": "/canvaskit/:path*" },
    { "source": "/flutter_bootstrap.js", "destination": "/flutter_bootstrap.js" },
    { "source": "/flutter.js", "destination": "/flutter.js" },
    { "source": "/main.dart.js", "destination": "/main.dart.js" },
    { "source": "/:path*.js", "destination": "/:path*.js" },
    { "source": "/:path*.json", "destination": "/:path*.json" },
    { "source": "/favicon.png", "destination": "/favicon.png" },
    { "source": "/manifest.json", "destination": "/manifest.json" },
    { "source": "/icons/:path*", "destination": "/icons/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
